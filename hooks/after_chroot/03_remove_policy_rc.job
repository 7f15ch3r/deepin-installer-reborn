# delete policy-rc.d script if it already exist.

RC="/target/usr/sbin/policy-rc.d"

if [ -f "$RC" ]; then
    echo "[INFO] policy-rc.d exists.Will remove it..."
    rm "$RC"
else
    echo "[INFO] policy-rc.d not exists"
fi

