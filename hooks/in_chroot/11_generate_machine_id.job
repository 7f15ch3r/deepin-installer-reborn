# try to fix systemd machine-id config at first boot

oldMachineID="/deepinhost/var/cache/machine-id"
if [ -f $oldMachineID ] ;then
    cp $oldMachineID /etc/
else
    systemd-machine-id-setup
fi

chmod 644 /etc/machine-id
